{
  "name": "app",
  "version": "1.0.0",
  "description": "API con Node, Express, TypeScript, Swagger, Jest y Sequelize para PostgreSQL",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest --passWithNoTests",
    "seed": "ts-node src/scripts/runSeeders.ts",
    "sync-seed": "ts-node src/scripts/syncAndSeed.ts",
    "create-tables": "ts-node src/scripts/createTables.ts"
  },
  "dependencies": {
    "express": "^4.19.2",
    "pg": "^8.11.5",
    "sequelize": "^6.37.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "@types/bcrypt": "^6.0.0",
    "bcrypt": "^6.0.0",
    "csv-parser": "^3.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}